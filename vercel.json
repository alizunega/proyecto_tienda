{
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "http://localhost:3001/products"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ]
}